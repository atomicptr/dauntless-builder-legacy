{
    "name": "dauntless-builder",
    "version": "2.0.0",
    "type": "module",
    "repository": "git@github.com:atomicptr/dauntless-builder",
    "author": "Christopher Kaster <me@atomicptr.de>",
    "license": "AGPLv3",
    "private": true,
    "scripts": {
        "build": "yarn build:deps && yarn build:frontend",
        "build:deps": "ts-node -P ./builder/tsconfig.json ./builder/src/main.ts",
        "build:data": "yarn build:deps --run data i18n",
        "build:frontend": "vite build",
        "dev": "vite dev",
        "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
        "deploy": "yarn build && ./scripts/deploy.sh",
        "lint": "yarn lint:json && eslint .",
        "lint:json": "ts-node -P ./scripts/tsconfig.json ./scripts/json-sort.ts",
        "lint:fix": "prettier . --write && yarn lint:fix:json && eslint . --fix",
        "lint:fix:json": "yarn lint:json --fix"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": "please-use-yarn",
        "yarn": ">=1.20.0"
    },
    "dependencies": {
        "@emotion/react": "^11.10.0",
        "@emotion/styled": "^11.10.0",
