        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "react-virtualized-auto-sizer": "^1.0.6",
        "react-window": "^1.8.7",
        "ts-pattern": "^4.0.5"
    },
    "devDependencies": {
        "@crowdin/crowdin-api-client": "^1.19.0",
        "@types/command-line-args": "^5.2.0",
        "@types/glob": "^7.2.0",
        "@types/jest": "^28.1.7",
        "@types/js-yaml": "^4.0.5",
        "@types/md5": "^2.3.2",
        "@types/node": "^18.7.8",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@types/react-helmet": "^6.1.5",
