    "prettier": "^3.0.3",
    "queue-promise": "^2.2.1",
    "sort-json": "^2.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.5",
    "yauzl": "^2.10.0"
  }
}
