{
    "name": "dauntless-builder",
    "version": "2.0.0",
    "type": "module",
    "repository": "git@github.com:atomicptr/dauntless-builder",
    "author": "Christopher Kaster <me@atomicptr.de>",
    "license": "AGPLv3",
    "private": true,
    "scripts": {
        "build": "yarn build:deps && yarn build:frontend",
        "build:deps": "ts-node -P ./builder/tsconfig.json ./builder/src/main.ts",
        "build:data": "yarn build:deps --run data i18n",
        "build:frontend": "vite build",
        "dev": "vite dev",
        "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
        "deploy": "yarn build && ./scripts/deploy.sh",
        "lint": "yarn lint:json && eslint .",
        "lint:json": "scripts/json-linter -r src tsconfig.json .eslintrc",
        "lint:fix": "prettier . --write && yarn lint:fix:json && eslint . --fix",
        "lint:fix:json": "yarn lint:json --fix"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": "please-use-yarn",
        "yarn": ">=1.20.0"
    },
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.11.0",
        "@mui/material": "^5.11.0",
        "@reduxjs/toolkit": "^1.9.1",
        "hashids": "^2.2.10",
        "i18next": "<22",
        "i18next-browser-languagedetector": "^7.0.1",
        "i18next-resources-to-backend": "^1.1.1",
        "md5": "^2.3.0",
        "notistack": "^2.0.8",
        "react": "^18.2.0",
        "react-country-flag": "^3.0.2",
        "react-dom": "^18.2.0",
        "react-helmet": "^6.1.0",
        "react-i18next": "^12.1.1",
        "react-icons": "^4.7.1",
        "react-lazy-load-image-component": "^1.5.6",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.4.5",
        "react-virtualized-auto-sizer": "^1.0.7",
        "react-window": "^1.8.8",
        "ts-pattern": "^4.0.6"
    },
    "devDependencies": {
        "@crowdin/crowdin-api-client": "^1.19.2",
        "@types/command-line-args": "^5.2.0",
        "@types/glob": "^8.0.0",
        "@types/jest": "^29.2.4",
        "@types/js-yaml": "^4.0.5",
        "@types/md5": "^2.3.2",
        "@types/node": "^18.11.14",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@types/react-helmet": "^6.1.6",
        "@types/react-lazy-load-image-component": "^1.5.2",
        "@types/react-redux": "^7.1.24",
        "@types/react-virtualized-auto-sizer": "^1.0.1",
        "@types/react-window": "^1.8.5",
        "@types/sort-json": "^2.0.1",
        "@types/yauzl": "^2.10.0",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "@vitejs/plugin-react": "^3.0.0",
        "ajv": "^8.11.2",
        "axios": "^1.2.1",
        "command-line-args": "^5.2.1",
        "eslint": "^8.29.0",
        "eslint-plugin-i18next": "^6.0.0-6",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^8.0.0",
