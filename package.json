        "hashids": "^2.2.10",
        "i18next": "^21.9.1",
        "i18next-browser-languagedetector": "^6.1.5",
        "i18next-resources-to-backend": "^1.0.0",
        "md5": "^2.3.0",
        "notistack": "^2.0.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet": "^6.1.0",
        "react-i18next": "^11.18.4",
        "react-icons": "^4.4.0",
