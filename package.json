{
    "name": "dauntless-builder",
    "version": "2.0.0",
    "type": "module",
    "repository": "git@github.com:atomicptr/dauntless-builder",
    "author": "Christopher Kaster <me@atomicptr.de>",
    "license": "AGPLv3",
    "private": true,
    "scripts": {
        "build": "npm run build:deps && npm run build:frontend",
        "build:deps": "tsx --tsconfig ./builder/tsconfig.json ./builder/src/main.ts",
        "build:data": "npm run build:deps -- --run data i18n build-finder-intermediate-data",
        "build:frontend": "vite build",
        "dev": "vite dev",
        "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
        "bench": "npm run benchmark",
        "benchmark": "npm run test -- --projects jest-benchmark.config.ts",
        "deploy": "npm run build && ./scripts/deploy.sh",
        "lint": "npm run lint:json && eslint .",
        "lint:json": "scripts/json-linter -r src tsconfig.json .eslintrc",
        "lint:fix": "prettier . --write && npm run lint:fix:json && eslint . --fix",
        "lint:fix:json": "npm run lint:json -- --fix"
    },
    "engines": {
        "node": ">=18.0.0",
        "yarn": "please-use-npm"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.14",
        "@mui/material": "^5.14.14",
        "@reduxjs/toolkit": "^1.9.7",
        "hashids": "^2.3.0",
        "i18next": "^23",
        "i18next-browser-languagedetector": "^7.1.0",
        "i18next-resources-to-backend": "^1.1.4",
        "md5": "^2.3.0",
        "notistack": "^3.0.1",
        "react": "^18.2.0",
        "react-country-flag": "^3.1.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^4.0.11",
        "react-helmet": "^6.1.0",
        "react-i18next": "^13.3.1",
        "react-icons": "^4.11.0",
        "react-lazy-load-image-component": "^1.6.0",
        "react-redux": "^8.1.3",
        "react-router-dom": "^6.17.0",
        "react-virtualized-auto-sizer": "^1.0.20",
        "react-window": "^1.8.9",
        "ts-pattern": "^5.0.5",
        "vite-plugin-favicon": "^1.0.8"
    },
    "devDependencies": {
        "@crowdin/crowdin-api-client": "^1.25.0",
        "@types/command-line-args": "^5.2.2",
        "@types/glob": "^8.1.0",
        "@types/jest": "^29.5.6",
        "@types/js-yaml": "^4.0.8",
        "@types/md5": "^2.3.4",
        "@types/node": "^20.8.7",
        "@types/react": "^18.2.30",
        "@types/react-dom": "^18.2.14",
        "@types/react-helmet": "^6.1.8",
        "@types/react-lazy-load-image-component": "^1.6.2",
        "@types/react-redux": "^7.1.28",
        "@types/react-virtualized-auto-sizer": "^1.0.2",
        "@types/react-window": "^1.8.7",
        "@types/sort-json": "^2.0.2",
        "@types/yauzl": "^2.10.2",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "@vitejs/plugin-react": "^4.1.0",
        "ajv": "^8.12.0",
        "axios": "^1.5.1",
        "command-line-args": "^5.2.1",
        "eslint": "^8.51.0",
        "eslint-plugin-i18next": "^6.0.3",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-unused-imports": "^3.0.0",
        "glob": "^10.3.10",
        "jest": "^29",
        "jest-bench": "^29",
        "jest-environment-jsdom": "^29.7.0",
        "js-yaml": "^4.1.0",
        "license-checker": "^25.0.1",
        "prettier": "^3.0.3",
        "queue-promise": "^2.2.1",
        "sort-json": "^2.0.1",
        "ts-jest": "^29",
        "tsx": "^3.14.0",
        "typescript": "^5.2.2",
        "vite": "^4.5.0",
        "vite-plugin-pwa": "^0.16.5",
        "yauzl": "^2.10.0"
    }
}
