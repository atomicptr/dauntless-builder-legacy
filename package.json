{
  "name": "dauntless-builder",
  "version": "2.0.0",
  "type": "module",
  "repository": "git@github.com:atomicptr/dauntless-builder",
  "author": "Christopher Kaster <me@atomicptr.de>",
  "license": "AGPLv3",
  "private": true,
  "scripts": {
    "build": "bun build:deps && bun build:frontend",
    "build:deps": "bun ./builder/src/main.ts",
    "build:data": "bun build:deps --run data i18n build-finder-intermediate-data",
    "build:frontend": "bun x vite build",
    "deploy": "bun run build && ./scripts/deploy.sh",
    "benchmark": "exit 0",
    "lint": "bun lint:json && bun x eslint .",
    "lint:json": "scripts/json-linter -r src tsconfig.json .eslintrc",
    "lint:fix": "prettier . --write && bun lint:fix:json && eslint . --fix",
    "lint:fix:json": "bun lint:json --fix"
  },
  "engines": {
    "npm": "please-use-bun",
    "yarn": "please-use-bun",
    "bun": "^1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.8",
    "@mui/material": "^5.14.8",
    "@reduxjs/toolkit": "^1.9.5",
    "hashids": "^2.3.0",
    "i18next": "^23",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-resources-to-backend": "^1.1.4",
    "md5": "^2.3.0",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^13.2.2",
    "react-icons": "^4.11.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "ts-pattern": "^5.0.5"
  },
  "devDependencies": {
    "@crowdin/crowdin-api-client": "^1.24.0",
    "@types/command-line-args": "^5.2.1",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/md5": "^2.3.2",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@types/react-lazy-load-image-component": "^1.6.0",
    "@types/react-redux": "^7.1.26",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/sort-json": "^2.0.1",
    "@types/yauzl": "^2.10.0",
    "@types/yauzl-promise": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.0.4",
    "ajv": "^8.12.0",
    "axios": "^1.5.0",
    "bun-types": "^1.0.1",
    "command-line-args": "^5.2.1",
    "eslint": "^8.49.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
